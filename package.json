  "devDependencies": {
    "husky": "^4.0.0-beta.5",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2"
  },
  "keywords": [
    "gatsby",
    "documention"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && gatsby build",
    "build:production": "yarn clean && gatsby build --prefix-paths",
    "clean": "yarn reset && gatsby clean",
    "develop": "gatsby develop",
    "prettier": "prettier --write \"src/**/*.{js,jsx,json,md,mdx}\"",
    "prettier:check": "prettier --check \"src/**/*.{js,jsx,json,md,mdx}\"",
    "reset": "rimraf src/external/*",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "yarn prettier:check"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jcalcaben/gatsby-doc-site"
  },
  "bugs": {
    "url": "https://github.com/jcalcaben/gatsby-doc-site/issues"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn prettier:check"
    }
  }
}
