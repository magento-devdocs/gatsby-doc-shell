  "author": "James Calcaben <jcalcaben@gmail.com>",
  "dependencies": {
    "@adobe/spectrum-css-workflow-icons": "^1.0.0",
    "@mdx-js/mdx": "^1.5.1",
    "@mdx-js/react": "^1.5.1",
