    "prism-themes": "^1.3.0",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "remark-grid-tables": "^1.7.1"
